<section class="body">
  <h1><%= gettext "All the Monologues", name: "All the Monologues" %></h1>
</section>

<section class="row">
  <article class="monologue_preview">
    <h2>All the Monologues from All the Plays</h2>
     <%= for row <- @rows do %>
       <br><br>
       <%= row.play %>, <%= row.character %>, <%= row.firstline %>, <%= row.location %>
      <br>
       <%= raw(row.body) %>
     <% end %>
  </article>
</section>
